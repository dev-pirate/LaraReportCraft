{
    "name": "dev-pirate/lara-report-craft",
    "description": "Laravel package to create and manage reports easily",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DevPirate\\LaraReportCraft\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Meherbs",
            "email": "meherbensalah4@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "mockery/mockery": "^1.2"
    },
    "extra": {
        "laravel": {
            "aliases": {
                "LaraReportCraft": "DevPirate\\LaraReportCraft\\LaraReportCraft"
            },
            "providers": [
              "DevPirate\\LaraReportCraft\\Providers\\LaraReportCraftProvider"
            ]
        }
    }
}
